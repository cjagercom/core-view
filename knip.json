{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/routes/**/*.{ts,tsx}",
    "scripts/**/*.{ts,mjs}"
  ],
  "project": ["app/**/*.{ts,tsx}", "scripts/**/*.{ts,mjs}"],
  "ignore": [".react-router/**", "build/**"],
  "ignoreDependencies": ["tailwindcss"],
  "vitest": {
    "entry": ["tests/**/*.{test,spec}.{ts,tsx}", "app/**/*.{test,spec}.{ts,tsx}"]
  }
}
